{
    "name": "dcodegroup/ui-presets",
    "description": "DCODE preset for Laravel.",
    "keywords": [
        "dcode",
        "laravel",
        "preset"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "DCode Group"
        }
    ],
    "require": {
        "laravel/ui": "^3.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Dcode\\UiPresets\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "UiPresets\\Tall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel"],
        "laravel": {
            "providers": [
                "Dcode\\UiPresets\\DcodeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
